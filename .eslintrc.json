{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends":[
      "airbnb-base",
      "prettier"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
      "import/prefer-default-export": "off",
      "class-methods-use-this": "off",
      "import/extensions": "off",
      "consistent-return": "off",
      "no-console": "off",
      "semi": ["error", "always"]
    }
}
